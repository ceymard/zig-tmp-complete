{
	"name": "zig-tmp-complete",
	"displayName": "zig-tmp-complete",
	"description": "Temporary completion for the zig language while we wait for the compiler to implement RLS.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [
		"onLanguage:zig"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^12.7.12",
		"@types/vscode": "^1.38.0",
		"@types/which": "^1.3.1",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"chalk": "^2.4.2",
		"which": "^2.0.1"
	}
}
